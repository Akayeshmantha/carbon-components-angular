{
  "name": "@peretz/neutrino",
  "version": "1.2.0",
  "description": "Next generation components",
  "main": "index.js",
  "scripts": {
	"build": "bash scripts/build.sh",
	"demo:build": "gulp demo && webpack --config webpack.config.js",
	"demo:server": "gulp demo && webpack-dev-server --config webpack.config.js --watch",
	"demo:server:travis": "npm run demo:server &",
	"docs:build": "compodoc src -p tsconfig.json",
	"docs:server": "compodoc -s",
	"docs:run": "compodoc src -p tsconfig.json -s",
	"lint": "tslint -e spec.ts src/**/*.ts && stylelint src/**/*.scss --config .stylelintrc.yml",
	"test": "karma start",
	"test:a11y": "karma start karma.a11y.conf.js",
	"test:a11y:travis": "npm run demo:server:travis && wait-on http-get://localhost:9000/neutrino.umd.js && npm run test:a11y",
	"test:watch": "karma start --auto-watch --no-single-run",
	"utils:add": "git subtree add --prefix=src/utils git@github.ibm.com:peretz/utils.git master --squash",
	"utils:update": "git subtree pull --prefix=src/utils git@github.ibm.com:peretz/utils.git master --squash",
	"prepush": "bash scripts/prepush.sh"
  },
  "repository": {
	"type": "git",
	"url": "git@github.ibm.com:peretz/next.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "IBM",
